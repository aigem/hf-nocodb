#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules/tailwindcss/lib/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules/tailwindcss/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules/tailwindcss/lib/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules/tailwindcss/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.20_postcss@8.4.47__postcss@8.4.47/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
