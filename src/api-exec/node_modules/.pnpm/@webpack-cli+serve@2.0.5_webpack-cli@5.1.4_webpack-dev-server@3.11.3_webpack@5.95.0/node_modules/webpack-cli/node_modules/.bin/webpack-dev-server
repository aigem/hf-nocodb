#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules/webpack-dev-server/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules/webpack-dev-server/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@5.1.4_webpack@5.95.0/node_modules:/mnt/f/aiSOFT/aigem/hf/nocodb/hf-nocodb/src/api-exec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
